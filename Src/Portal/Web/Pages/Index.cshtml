@page
@using Interface
@model Web.Pages.IndexPageModel

<div>
    <table class="table table-striped table-sm table-bordered">
        <thead>
        <tr>
        <th>First Name</th>
        <th>LastName Name</th>
        </tr>
        </thead>
        <tbody>
        @foreach (User user in Model.Users)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
            </tr>
        }
        </tbody>
    </table>
</div>
